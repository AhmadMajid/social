<div class="col-7">
  <div class="mb-3">
    <%= link_to "Home", dashboard_path, class: "fs-4 text-decoration-none text-black fw-semibold" %>
  </div>
  <div class="mb-4">
    <%= render "shared/vibration_form", form_submit_path: vibrations_path, button_label: "Vibration" %>
  </div>
  <div
    id="vibrations"
    data-controller="dashboard-vibrations"
    data-next-page="<%= @vibration_activities_data[:next_page] %>"
    data-last-page="<%= @vibration_activities_data[:last_page] %>"
  >
<%= render partial: "vibrations/vibration", collection: @vibration_activities_data[:vibration_activities], as: :vibration_presenter %>
  </div>
</div>
<div class="col">
  <%= render "shared/hashtag_sidebar" %>
</div>
