<h1>We are logged in!</h1>
<%= link_to "Log out", destroy_user_session_path, data: { "turbo-method": :delete }, class: "btn btn-primary btn-lg" %>

<%= form_with model: Vibration.new, url: vibrations_path, local: true do |f| %>
<%= f.text_field :body %>
<%= f.submit %>
<% end %>

<ul>
  <% Vibration.find_each do |vibration| %>
    <li><%= vibration.body %></li>
  <% end %>
</ul>
