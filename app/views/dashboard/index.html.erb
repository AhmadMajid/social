<div class="col-7">
  <div class="mb-3">
    <%= link_to "Home", dashboard_path, class: "fs-4 text-decoration-none text-black fw-semibold" %>
  </div>
  <%= form_with model: Vibration.new, url: vibrations_path, method: :post, local: true, class: "w-100" do |f| %>
    <%= f.text_area :body, placeholder: "What's happening?", class: "form-control w-100 vibration-input mb-4" %>
    <div class="d-flex justify-content-end">
      <%= f.submit "Vibration", class: "btn btn-lg btn-social rounded-pill" %>
    </div>
  <% end %>
  <ul>
    <% Vibration.find_each do |vibration| %>
      <li><%= vibration.body %></li>
    <% end %>
  </ul>
</div>
<div class="col">
  Column
</div>
